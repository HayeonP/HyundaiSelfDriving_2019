<?xml version="1.0"?>
<package format="2">
  <name>nodelet_lidar_driver</name>
  <version>0.0.0</version>
  <description>The nodelet_lidar_driver package</description>
  <maintainer email="soorin@todo.todo">soorin</maintainer>
  <license>TODO</license>

  <build_depend>message_generation</build_depend>
